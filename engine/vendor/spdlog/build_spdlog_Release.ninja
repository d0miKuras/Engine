# project build file
# generated with premake ninja

ninja_required_version = 1.6

# core rules for Release
CFLAGS= -MD -MP -m64 -O2 -DSPDLOG_COMPILED_LIB -D_CRT_SECURE_NO_WARNINGS -DSPDLOG_WCHAR_TO_UTF8_SUPPORT -Iinclude
rule cc
  command = C:/Program Files/LLVM/bin/clang.exe $CFLAGS -x c -MF $out.d -c -o $out $in
  description = cc $out
  depfile = $out.d
  deps = gcc

CXXFLAGS= -MD -MP -m64 -O2 -DSPDLOG_COMPILED_LIB -D_CRT_SECURE_NO_WARNINGS -DSPDLOG_WCHAR_TO_UTF8_SUPPORT -Iinclude
rule cxx
  command = C:/Program Files/LLVM/bin/clang++.exe $CXXFLAGS -x c++ -MF $out.d -c -o $out $in
  description = cxx $out
  depfile = $out.d
  deps = gcc

RESFLAGS =  -DSPDLOG_COMPILED_LIB -D_CRT_SECURE_NO_WARNINGS -DSPDLOG_WCHAR_TO_UTF8_SUPPORT
rule rc
  command = windres -i $in -o $out $RESFLAGS
  description = rc $out

rule ar
  command = ar rcs $out $in
  description = ar $out

rule copy
  command = copy /B /Y $in $out
  description = copy $in $out

rule custom_command
  command = $CUSTOM_COMMAND
  description = $CUSTOM_DESCRIPTION

# build files
build bin-int/Release-windows-x86_64/vendor/spdlog/async.o: cxx vendor/spdlog/src/async.cpp
build bin-int/Release-windows-x86_64/vendor/spdlog/bundled_fmtlib_format.o: cxx vendor/spdlog/src/bundled_fmtlib_format.cpp
build bin-int/Release-windows-x86_64/vendor/spdlog/cfg.o: cxx vendor/spdlog/src/cfg.cpp
build bin-int/Release-windows-x86_64/vendor/spdlog/color_sinks.o: cxx vendor/spdlog/src/color_sinks.cpp
build bin-int/Release-windows-x86_64/vendor/spdlog/file_sinks.o: cxx vendor/spdlog/src/file_sinks.cpp
build bin-int/Release-windows-x86_64/vendor/spdlog/spdlog.o: cxx vendor/spdlog/src/spdlog.cpp
build bin-int/Release-windows-x86_64/vendor/spdlog/stdout_sinks.o: cxx vendor/spdlog/src/stdout_sinks.cpp

# link static lib
build bin/Release-windows-x86_64/vendor/spdlog/spdlog.lib: ar bin-int/Release-windows-x86_64/vendor/spdlog/async.o bin-int/Release-windows-x86_64/vendor/spdlog/bundled_fmtlib_format.o bin-int/Release-windows-x86_64/vendor/spdlog/cfg.o bin-int/Release-windows-x86_64/vendor/spdlog/color_sinks.o bin-int/Release-windows-x86_64/vendor/spdlog/file_sinks.o bin-int/Release-windows-x86_64/vendor/spdlog/spdlog.o bin-int/Release-windows-x86_64/vendor/spdlog/stdout_sinks.o

build spdlog_Release: phony bin/Release-windows-x86_64/vendor/spdlog/spdlog.lib
